import React, { Component } from 'react';
import { connect } from 'react-redux';
import { VendorMenuView} from '../VendorMenu';
import { getMenuStoreList } from '../../../actions/vendor.action';

function mapStateToProps(state) {

    return {
        vendorState : state.vendorState
    };
}

function mapDispatchToProps(dispatch) {

    return {
        // getVendorState: () => dispatch(getMenuVendorList()),
        getMenuStoreListDispatch: (data) => { dispatch(getMenuStoreList(data))}
    }
}


class StoreMenu extends Component {

    constructor(){

        super();
        this.getMenuStoreList = this.getMenuStoreList.bind(this);


    }

    componentDidMount = () => {

        this.getMenuStoreList();

    }

    componentDidUpdate = (prevProps) => {

        const { vendorState } = this.props;
    }

    getMenuStoreList = () => {
        const { getMenuStoreListDispatch } = this.props;

        console.log(this.props);
        
        const data = [
            { id :1 },
            { id :2 }
        ]

        data.map((item)=> {
            getMenuStoreListDispatch(item.id);
        })
    }
    render() {
        return (
            <VendorMenuView
                {...this.state}
                {...this.props}
            />
        )
    }
}

export default connect(
    mapStateToProps,
    mapDispatchToProps
)(StoreMenu);